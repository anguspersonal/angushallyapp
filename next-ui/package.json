{
  "name": "next-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ../scripts/check-port.js 3000 && next dev",
    "dev:next": "next dev",
    "dev:turbopack": "node ../scripts/check-port.js 3000 && next dev --turbopack",
    "dev:next:turbopack": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "fix-nextjs": "node ../scripts/fix-nextjs-build-errors.js",
    "fix-nextjs:clean": "node ../scripts/fix-nextjs-build-errors.js --clean-deps",
    "clean": "rm -rf .next node_modules package-lock.json",
    "rebuild": "npm run clean && npm install && npm run dev",
    "optimize-css": "node scripts/optimize-css.js"
  },
  "dependencies": {
    "@mantine/carousel": "^8.1.3",
    "@mantine/charts": "^8.1.3",
    "@mantine/core": "^8.1.3",
    "@mantine/form": "^8.1.3",
    "@mantine/notifications": "^8.1.3",
    "@react-oauth/google": "^0.12.2",
    "@tabler/icons-react": "^3.34.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vis.gl/react-google-maps": "^1.5.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.9",
    "critters": "^0.0.25",
    "embla-carousel-autoplay": "^8.6.0",
    "framer-motion": "^12.23.0",
    "next": "^15.3.5",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.13",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.3.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^24.0.0",
    "vitest": "^1.2.1"
  }
