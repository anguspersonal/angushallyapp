{
  "name": "angushallyapp-server",
  "version": "1.0.0",
  "private": true,
  "description": "Server component for angushallyapp",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "openai": "^4.100.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
