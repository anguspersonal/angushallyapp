{
  "name": "heroku-cra-node",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "node server",
    "build": "cd react-ui/ && npm install && npm run build",
    "dev": "cd react-ui/ && npm install && npm start",
    "test": "npx jest server/tests",
    "test--verbose": "npx jest server/tests --verbose"
  },
  "type": "commonjs",
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@mantine/carousel": "^7.17.4",
    "@vis.gl/react-google-maps": "^1.5.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.8.2",
    "fuse.js": "^7.0.0",
    "google-auth-library": "^9.15.1",
    "knex": "^3.1.0",
    "nodemailer": "^6.10.0",
    "npm": "^11.2.0",
    "pg": "^8.15.6",
    "puppeteer": "^24.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-markdown": "^9.1.0",
    "remark-gfm": "^4.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.17.0",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.15",
    "supertest": "^7.0.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "main": "index.js",
  "author": "angushally",
  "bugs": {
    "url": "https://github.com/mars/heroku-cra-node/issues"
  },
  "homepage": "https://github.com/mars/heroku-cra-node#readme"
}
